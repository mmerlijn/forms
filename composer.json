{
    "name": "mmerlijn/forms",
    "description": "Advanced laravel form application for health care",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "mmerlijn/blade-components": "dev-master",
        "mmerlijn/msg": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Menno",
            "email": "menno@mmerlijn.nl"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "illuminate/support": "8.x-dev",
        "orchestra/testbench": "6.x-dev",
        "phpunit/phpunit": "9.5.x-dev",
        "livewire/livewire": "dev-master",
        "spatie/laravel-permission": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers":[
                "mmerlijn\\forms\\FormsServiceProvider"
            ]

        }
    },
    "autoload": {
        "psr-4": {
            "mmerlijn\\forms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mmerlijn\\forms\\tests\\": "tests"
        }
    }
}
